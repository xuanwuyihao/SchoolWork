.nTab
 {
  width: 95%;
  height:auto;
  overflow: hidden;
 }
 .none
 {
  display: none;
 }
 .nTab .TabTitle li
 {
  float: left;
  cursor: pointer;
  height: 35px;
  line-height: 35px;
  font-weight: bold;
  text-align: center;
  width: 124px;
  list-style-type: none;
 }
 .nTab .TabTitle li a
 {
  text-decoration: none;
 }
 .nTab .TabTitle .active
 {
  color: orange;
  border-bottom: 2px solid orange;
  margin-top: 1px;
 }
 .nTab .TabTitle .normal
 {
 }
 .nTab .TabContent
 {
  clear: both;
  overflow: hidden;
  background: #fff;
  display: block;
 }
