       		 	<!-- 默认拦截器   主要是拦截myfile MyFileContentType MyFileFileName-->
   		 		<interceptor-ref name="fileUploadStack"/>
        		<param name="savePath">/Myfiles</param>
        		<result type="redirect">getCar.action</result>
			</action>
	
